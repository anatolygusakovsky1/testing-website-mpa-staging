#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/escodegen@1.14.3/node_modules/escodegen/bin/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/escodegen@1.14.3/node_modules/escodegen/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/escodegen@1.14.3/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules:/Users/shantanu.sharma/development/node_modules:/Users/shantanu.sharma/node_modules:/Users/node_modules:/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/static-eval@2.0.2/node_modules/static-eval/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/static-eval@2.0.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/escodegen@1.14.3/node_modules/escodegen/bin/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/escodegen@1.14.3/node_modules/escodegen/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/escodegen@1.14.3/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules:/Users/shantanu.sharma/development/node_modules:/Users/shantanu.sharma/node_modules:/Users/node_modules:/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/static-eval@2.0.2/node_modules/static-eval/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/static-eval@2.0.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../escodegen@1.14.3/node_modules/escodegen/bin/escodegen.js" "$@"
else
  exec node  "$basedir/../../../../../escodegen@1.14.3/node_modules/escodegen/bin/escodegen.js" "$@"
fi
