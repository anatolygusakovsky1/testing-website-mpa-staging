#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/css-has-pseudo@3.0.4_postcss@8.4.39/node_modules/css-has-pseudo/dist/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/css-has-pseudo@3.0.4_postcss@8.4.39/node_modules/css-has-pseudo/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/css-has-pseudo@3.0.4_postcss@8.4.39/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules:/Users/shantanu.sharma/development/node_modules:/Users/shantanu.sharma/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/css-has-pseudo@3.0.4_postcss@8.4.39/node_modules/css-has-pseudo/dist/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/css-has-pseudo@3.0.4_postcss@8.4.39/node_modules/css-has-pseudo/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/css-has-pseudo@3.0.4_postcss@8.4.39/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules:/Users/shantanu.sharma/development/node_modules:/Users/shantanu.sharma/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.cjs" "$@"
else
  exec node  "$basedir/../../dist/cli.cjs" "$@"
fi
