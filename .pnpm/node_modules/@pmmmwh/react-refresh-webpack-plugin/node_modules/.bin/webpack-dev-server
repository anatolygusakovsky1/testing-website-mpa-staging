if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../webpack-dev-server@4.15.2_webpack@5.92.1/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
