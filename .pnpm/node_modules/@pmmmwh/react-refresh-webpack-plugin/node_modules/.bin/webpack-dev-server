#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.92.1/node_modules/webpack-dev-server/bin/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.92.1/node_modules/webpack-dev-server/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.92.1/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules:/Users/shantanu.sharma/development/node_modules:/Users/shantanu.sharma/node_modules:/Users/node_modules:/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.15_c89143abe04b9b3d734616bbb28287d7/node_modules/@pmmmwh/react-refresh-webpack-plugin/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.15_c89143abe04b9b3d734616bbb28287d7/node_modules/@pmmmwh/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.15_c89143abe04b9b3d734616bbb28287d7/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.92.1/node_modules/webpack-dev-server/bin/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.92.1/node_modules/webpack-dev-server/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.92.1/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules:/Users/shantanu.sharma/development/node_modules:/Users/shantanu.sharma/node_modules:/Users/node_modules:/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.15_c89143abe04b9b3d734616bbb28287d7/node_modules/@pmmmwh/react-refresh-webpack-plugin/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.15_c89143abe04b9b3d734616bbb28287d7/node_modules/@pmmmwh/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.15_c89143abe04b9b3d734616bbb28287d7/node_modules"
