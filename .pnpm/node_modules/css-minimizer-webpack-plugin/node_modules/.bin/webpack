#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/webpack@5.92.1/node_modules/webpack/bin/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/webpack@5.92.1/node_modules/webpack/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/webpack@5.92.1/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules:/Users/shantanu.sharma/development/node_modules:/Users/shantanu.sharma/node_modules:/Users/node_modules:/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/css-minimizer-webpack-plugin@3.4.1_webpack@5.92.1/node_modules/css-minimizer-webpack-plugin/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/css-minimizer-webpack-plugin@3.4.1_webpack@5.92.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/webpack@5.92.1/node_modules/webpack/bin/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/webpack@5.92.1/node_modules/webpack/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/webpack@5.92.1/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules:/Users/shantanu.sharma/development/node_modules:/Users/shantanu.sharma/node_modules:/Users/node_modules:/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/css-minimizer-webpack-plugin@3.4.1_webpack@5.92.1/node_modules/css-minimizer-webpack-plugin/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/css-minimizer-webpack-plugin@3.4.1_webpack@5.92.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack@5.92.1/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../../../../../webpack@5.92.1/node_modules/webpack/bin/webpack.js" "$@"
fi
