#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/fixtures/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/import-local@3.1.0/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules:/Users/shantanu.sharma/development/node_modules:/Users/shantanu.sharma/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/fixtures/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/import-local@3.1.0/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules:/Users/shantanu.sharma/development/node_modules:/Users/shantanu.sharma/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../fixtures/cli.js" "$@"
else
  exec node  "$basedir/../../fixtures/cli.js" "$@"
fi
