#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/jake@10.9.1/node_modules/jake/bin/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/jake@10.9.1/node_modules/jake/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/jake@10.9.1/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules:/Users/shantanu.sharma/development/node_modules:/Users/shantanu.sharma/node_modules:/Users/node_modules:/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/ejs@3.1.10/node_modules/ejs/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/ejs@3.1.10/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/jake@10.9.1/node_modules/jake/bin/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/jake@10.9.1/node_modules/jake/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/jake@10.9.1/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules:/Users/shantanu.sharma/development/node_modules:/Users/shantanu.sharma/node_modules:/Users/node_modules:/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/ejs@3.1.10/node_modules/ejs/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/ejs@3.1.10/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jake@10.9.1/node_modules/jake/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../jake@10.9.1/node_modules/jake/bin/cli.js" "$@"
fi
