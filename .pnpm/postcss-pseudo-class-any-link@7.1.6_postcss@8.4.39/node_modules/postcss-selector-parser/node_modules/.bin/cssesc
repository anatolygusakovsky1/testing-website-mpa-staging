#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/cssesc@3.0.0/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules:/Users/shantanu.sharma/development/node_modules:/Users/shantanu.sharma/node_modules:/Users/node_modules:/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/postcss-selector-parser@6.1.0/node_modules/postcss-selector-parser/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/postcss-selector-parser@6.1.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/cssesc@3.0.0/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules:/Users/shantanu.sharma/development/node_modules:/Users/shantanu.sharma/node_modules:/Users/node_modules:/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/postcss-selector-parser@6.1.0/node_modules/postcss-selector-parser/node_modules:/Users/shantanu.sharma/development/testing-website-mpa/node_modules/.pnpm/postcss-selector-parser@6.1.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
else
  exec node  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
fi
